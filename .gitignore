# This .gitignore file should be placed at the root of your Unity project directory
#
# Get latest from https://github.com/github/gitignore/blob/main/Unity.gitignore
#
/DungeonDiver/[Ll]ibrary/
/DungeonDiver/[Tt]emp/
/DungeonDiver/[Oo]bj/
/DungeonDiver/[Bb]uild/
/DungeonDiver/[Bb]uilds/
/DungeonDiver/[Ll]ogs/
/DungeonDiver/[Uu]ser[Ss]ettings/

# MemoryCaptures can get excessive in size.
# They also could contain extremely sensitive data
/DungeonDiver/[Mm]emoryCaptures/

# Recordings can get excessive in size
/DungeonDiver/[Rr]ecordings/

# Uncomment this line if you wish to ignore the asset store tools plugin
# /[Aa]ssets/AssetStoreTools*

# Autogenerated Jetbrains Rider plugin
/DungeonDiver/[Aa]ssets/Plugins/Editor/JetBrains*

# Visual Studio cache directory
/DungeonDiver.vs/

# Gradle cache directory
/DungeonDiver.gradle/

# Autogenerated VS/MD/Consulo solution and project files
/DungeonDiverExportedObj/
/DungeonDiver.consulo/
/DungeonDiver*.csproj
/DungeonDiver*.unityproj
/DungeonDiver*.sln
/DungeonDiver*.suo
/DungeonDiver*.tmp
/DungeonDiver*.user
/DungeonDiver*.userprefs
/DungeonDiver*.pidb
/DungeonDiver*.booproj
/DungeonDiver*.svd
/DungeonDiver*.pdb
/DungeonDiver*.mdb
/DungeonDiver*.opendb
/DungeonDiver*.VC.db

# Unity3D generated meta files
/DungeonDiver*.pidb.meta
/DungeonDiver*.pdb.meta
/DungeonDiver*.mdb.meta

# Unity3D generated file on crash reports
/DungeonDiversysinfo.txt

# Builds
/DungeonDiver*.apk
/DungeonDiver*.aab
/DungeonDiver*.unitypackage
/DungeonDiver*.app

# Crashlytics generated file
/DungeonDivercrashlytics-build.properties

# Packed Addressables
/DungeonDiver/[Aa]ssets/[Aa]ddressable[Aa]ssets[Dd]ata/*/*.bin*

# Temporary auto-generated Android Assets
/DungeonDiver/[Aa]ssets/[Ss]treamingAssets/aa.meta
/DungeonDiver/[Aa]ssets/[Ss]treamingAssets/aa/*
